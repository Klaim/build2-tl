#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define TL_OPTIONAL_VERSION       $tl_optional.version.project_number$ULL
#define TL_OPTIONAL_VERSION_STR   "$tl_optional.version.project$"
#define TL_OPTIONAL_VERSION_ID    "$tl_optional.version.project_id$"
#define TL_OPTIONAL_VERSION_FULL  "$tl_optional.version$"

#define TL_OPTIONAL_VERSION_MAJOR $tl_optional.version.major$
#define TL_OPTIONAL_VERSION_MINOR $tl_optional.version.minor$
#define TL_OPTIONAL_VERSION_PATCH $tl_optional.version.patch$

#define TL_OPTIONAL_PRE_RELEASE   $tl_optional.version.pre_release$

#define TL_OPTIONAL_SNAPSHOT_SN   $tl_optional.version.snapshot_sn$ULL
#define TL_OPTIONAL_SNAPSHOT_ID   "$tl_optional.version.snapshot_id$"
